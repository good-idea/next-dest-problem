{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "app/static/$1"
    },
    { "src": "/favicon.ico", "dest": "app/favicon.ico" },
    {
      "src": "/(.*)",
      "dest": "app/$1",
      "continue": true
    }
  ]
}
